{
  "name": "hattie",
  "version": "0.1.0",
  "description": "A [Tailwind](https://tailwindcss.com/) + [Eleventy](https://www.11ty.dev/) + [Alpine](https://alpinejs.dev/) experiment.",
  "main": "index.js",
  "scripts": {
    "start:css": "npx tailwindcss -i src/css/input.css -o dist/css/output.css --watch",
    "start:11ty": "eleventy --serve",
    "start": "run-p start:*",
    "build:css": "NODE_ENV=production npx tailwindcss -i src/css/input.css -o dist/css/output.css --minify",
    "build:11ty": "eleventy",
    "build": "run-p build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zarahzachz/hattie.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zarahzachz/hattie/issues"
  },
  "homepage": "https://github.com/zarahzachz/hattie#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "luxon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.1.7"
  }
}
